{
  "manifest_version": 2,
  "name": "Fake News Launcher",
  "description": "Check the integrity of news!",
  "version": "1.1.0",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'sha256-1PR+N55Hkxu25E6OAz+/qiDloEvODa0hj23jo8GhjXM='; object-src 'self'"
}